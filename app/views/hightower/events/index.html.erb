<div class="row">
  <aside class="side-panel">
    <ul class="filter-options list-group">
      <%= render partial: @dashboard.events_tabs.tabs, as: :tab %>
    </ul>
  </aside>

  <section class="main-panel">
    <div class="page-header">
      <h1 class="page-title"><%= @dashboard.title %></h1>
    </div>
    <table class="table">
      <thead>
        <tr>
          <th>User</th>
          <th>Action</th>
          <th>Info</th>
          <th>Tracked at</th>
        </tr>
      </thead>
      <tbody>        
        <%= render partial: 'hightower/events/event', collection: @dashboard.paginated_events %>
      </tbody>
    </table>
    <%= paginate @dashboard.paginated_events %>
  </section>
</div>